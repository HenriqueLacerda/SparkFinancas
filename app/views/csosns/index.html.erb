<h1>Listing csosns</h1>

<table>
  <tr>
    <th>Codigo</th>
    <th>Nome</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @csosns.each do |csosn| %>
  <tr>
    <td><%= csosn.codigo %></td>
    <td><%= csosn.nome %></td>
    <td><%= link_to 'Show', csosn %></td>
    <td><%= link_to image_tag("alterar.png", :border => 0), edit_csosn_path(csosn) %></td>
    <td><%= link_to image_tag("remover.png", :border => 0), csosn, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
<%= will_paginate @csosns, :update => 'div_principal' %>
<br />
<div id="div_principal">	
	principal
</div>
<%= link_to 'New Csosn', new_csosn_path %>
